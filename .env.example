# Holded API Key (from Settings > Developers in Holded)
HOLDED_API_KEY=your-api-key-here

# PostgreSQL (for audit logging)
# Railway variable: ${{Postgres.DATABASE_URL}}
DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/holded_audit

# Redis (for caching)
# Railway variable: ${{Redis.REDIS_URL}}
REDIS_URL=redis://localhost:6379

# Feature flags
CACHE_ENABLED=true
AUDIT_ENABLED=true
